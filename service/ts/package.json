{
  "name": "openrating-api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "lint": "eslint .",
    "test": "NODE_ENV=test AUTH_DISABLE=1 node --test --loader tsx $(find ./test -name '*.test.ts' -o -name '*.test.js')",
    "db:generate": "drizzle-kit generate --config drizzle.config.ts",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:studio": "drizzle-kit studio --config drizzle.config.ts",
    "grants": "tsx scripts/grants.ts",
    "nightly": "NODE_ENV=production tsx scripts/nightly.ts",
    "replay": "tsx scripts/replay.ts",
    "insights:worker": "NODE_ENV=production tsx scripts/insights-worker.ts",
    "ai-insights:worker": "NODE_ENV=production tsx scripts/ai-insights-worker.ts",
    "leaderboard:backfill": "tsx scripts/backfill-leaderboard.ts",
    "token:mint:dev": "tsx scripts/mint-dev-token.ts"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.30.5",
    "express": "^4.19.2",
    "express-oauth2-jwt-bearer": "^1.7.0",
    "jose": "^5.9.3",
    "jsonwebtoken": "^9.0.2",
    "openai": "^4.58.0",
    "pg": "^8.11.5",
    "yargs": "^17.7.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.6",
    "@eslint/js": "^9.9.0",
    "@types/express": "^4.17.21",
    "@types/supertest": "^2.0.16",
    "@types/yargs": "^17.0.32",
    "drizzle-kit": "^0.30.2",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "globals": "^15.8.0",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.8.0"
  }
}
